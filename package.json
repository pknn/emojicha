{
  "name": "emojicha",
  "version": "1.0.1",
  "description": "CAPTCHA made with Emoji",
  "main": "index.ts",
  "author": "Pakanon Pantisawat <pakanon.pantisawat@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "rollup -c",
    "run:watch": "rollup -c -w",
    "run:examples": "cd examples && yarn build && yarn serve",
    "install:examples": "cd examples && yarn",
    "install:all": "yarn && yarn install:examples",
    "start": "npm-run-all --parallel run:watch run:examples"
  },
  "dependencies": {
    "jest": "^27.0.4",
    "react": "^16.10.2",
    "react-dom": "^16.10.2"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/jest": "^26.0.23",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "@types/styled-components": "^5.1.10",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^1.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.1",
    "rollup": "^2.51.1",
    "rollup-plugin-delete": "^2.0.0",
    "typescript": "^4.3.2"
  },
  "peerDependencies": {
    "jest": "^27.0.4",
    "react": "^16.10.2",
    "react-dom": "^16.10.2"
  }
}
